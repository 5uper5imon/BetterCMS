@using BetterCms.Module.Pages.Content.Resources
@using BetterCms.Module.Root.Content.Resources

@model BetterCms.Module.Pages.ViewModels.Seo.EditSeoViewModel

@if (!Model.IsInSitemap)
{
    <div class="bcms-messages-type-1">
        <ul class="bcms-success-messages" style="display: none">
        </ul>
        <ul class="bcms-info-messages" style="display: none">
        </ul>
        <ul class="bcms-warning-messages">
            <li>
                <a class="bcms-btn-close" id="bcms-seo-closeinfomessage">@RootGlobalization.Button_Close</a>
                @PagesGlobalization.EditSeo_Messages_NotInSitemapWarnMessage
            </li>
        </ul>
        <ul class="bcms-error-messages" style="display: none">
        </ul>
    </div>
}
