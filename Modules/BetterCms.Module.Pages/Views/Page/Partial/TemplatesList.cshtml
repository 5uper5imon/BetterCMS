<div class="bcms-templates-holder" data-bind="foreach: templatesList.displayedTemplates">
    <div class="bcms-grid-box bcms-grid-box-pages bcms-js-grid-box" data-bind="css: { 'bcms-grid-box-active': isActive, 'bcms-grid-box-notcompatible': isCircularToCurrent }">
        <div class="bcms-content-titles" data-bind="text: title"></div>

        <div class="bcms-grid-image-holder">
            <!-- ko if: previewThumbnailUrl || previewUrl -->
            <div class="bcms-grid-image">
                <img data-bind="attr: { src: previewThumbnailUrl || previewUrl, alt: title }" />
            </div>

            <div class="bcms-zoom-overlay bcms-preview-template" data-bind="click: previewImage"></div>}
            <!-- /ko -->
            <!-- ko ifnot: previewThumbnailUrl || previewUrl -->
            <svg class="bcms-grid-no-preview" viewBox="-237.5 336 138 121.5" enable-background="new -237.5 336 138 121.5">
                <path d="m-237 336h137v4h-137v-4" fill="#e6e6e6" />
                <g fill="#fff" stroke="#d7d4d4" stroke-linecap="square">
                    <path d="m-237 404h43v27h-43v-27z" />
                    <path d="m-190 404h43v27h-43v-27z" />
                    <path d="m-237 437h137v20h-137v-20z" />
                    <path d="m-143 404h43v27h-43v-27z" />
                    <path d="m-237 345h137v53h-137v-53z" />
                </g>
            </svg>
            <!-- /ko -->
        </div>

        <div class="bcms-btn-selection bcms-js-grid-box" data-bind="click: $root.templatesList.setActive">Select</div>
    </div>
</div>